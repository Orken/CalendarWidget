{
    "name": "orken/calendar-widget-cakephp3",
    "description": "CalendarWidget plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.0.0 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CalendarWidget\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CalendarWidget\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
